{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@authConnector/auth-connector": "<rootDir>/../libs/auth-connector/src",
    "@authConnector/auth-connector/(.*)": "<rootDir>/../libs/auth-connector/src/$1"
  }
}