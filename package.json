{
  "name": "squidward-node",
  "version": "0.0.1",
  "description": "Squid proxy network node",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production && npm run build:node",
    "build:node": "NODE_ENV=production nest build",
    "build:squid-auth-handler": "NODE_ENV=production nest build squid-auth-handler",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:squid-auth-handler": "nest build squid-auth-handler --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "consola": "^3.4.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "nest-winston": "^1.10.2",
    "nestjs-zod": "^5.0.1",
    "node-supervisord": "^1.0.6-rc.2",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.17.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6",
    "@types/command-line-args": "^5.2.3",
    "@types/compression": "^1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "command-line-args": "^6.0.1",
    "eslint": "^9.18.0",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@authConnector/auth-connector(|/.*)$": "<rootDir>/libs/auth-connector/src/$1"
    }
  }
}
